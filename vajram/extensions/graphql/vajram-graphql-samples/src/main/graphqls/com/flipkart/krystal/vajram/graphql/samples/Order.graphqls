type Order @entity{
    id: ID,
    orderItemNames: [String] @dataFetcher(vajramId: "GetOrderItemNames", subPackage: "order")
    name: String @dataFetcher(vajramId: "GetOrderItemNames", subPackage: "order")
    dummies: [Dummy] @idFetcher(vajramId: "GetDummyIds", subPackage: "order")
    hello: Hello
}

type Dummy @entity{
    id: ID,
    name: String
}

type Hello {
    toName: String
}